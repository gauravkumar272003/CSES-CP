#include <bits/stdc++.h>
using namespace std;
#define ll long long
 
int main()
{
    ll n;
    cin >> n;
 
    for (int i = 1; i <= n; i++)
    {
        // cout << (i * i * (i * i - 1) / 2 - 4 * (i - 1) * (i - 2)) << '\n';
 
        long a = ((long)i * i * (i * i - 1)) / 2;
        long b = 4 * (i - 1) * (i - 2);
        cout << a - b << '\n';
    }
    return 0;
}
